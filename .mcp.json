{
  "mcpServers": {
    "go-mcp": {
      "command": "npx",
      "args": [
        "mcp-remote",
        "https://gitmcp.io/golang/go/"
      ]
    },
    "typescript-mcp": {
      "command": "npx",
      "args": [
        "mcp-remote",
        "https://gitmcp.io/microsoft/TypeScript/"
      ]
    },
    "dotnet-mcp": {
      "command": "npx",
      "args": [
        "mcp-remote",
        "https://gitmcp.io/microsoft/dotnet/"
      ]
    },
    "dotnetcommunity-mcp": {
      "command": "npx",
      "args": [
        "mcp-remote",
        "https://gitmcp.io/CommunityToolkit/dotnet/"
      ]
    },
    "filesystem": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-filesystem",
        "/home/freyr/homeprojects/concrete"
      ]
    },
    "terraform": {
      "command": "docker",
      "args": [
        "run",
        "-i",
        "--rm",
        "hashicorp/terraform-mcp-server"
      ]
    }
  }
}