{
  "mcpServers": {
    "go-mcp": {
      "command": "npx",
      "args": [
        "mcp-remote",
        "https://gitmcp.io/golang/go/"
      ]
    },
    "typescript-mcp": {
      "command": "npx",
      "args": [
        "mcp-remote",
        "https://gitmcp.io/microsoft/TypeScript/"
      ]
    },
    "dotnet-mcp": {
      "command": "npx",
      "args": [
        "mcp-remote",
        "https://gitmcp.io/microsoft/dotnet/"
      ]
    },
    "pnpm-mcp": {
      "command": "npx",
      "args": [
        "mcp-remote",
        "https://gitmcp.io/pnpm/pnpm/"
      ]
    },
    "filesystem": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-filesystem",
        "/home/freyr/homeprojects/concrete"
      ]
    },
    "terraform": {
      "command": "docker",
      "args": [
        "run",
        "-i",
        "--rm",
        "hashicorp/terraform-mcp-server"
      ]
    },
    "memory-db": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-memory"
      ],
      "env": {
        "MEMORY_FILE_PATH": "/home/freyr/homeprojects/concrete/memory-db.json"
      }
    },
    "memory-app": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-memory"
      ],
      "env": {
        "MEMORY_FILE_PATH": "/home/freyr/homeprojects/concrete/memory-app.json"
      }
    }
  }
}